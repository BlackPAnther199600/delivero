{
  "expo": {
    "name": "Delivero",
    "slug": "delivero",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "52.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B00"
    },
    "android": {
      "package": "com.anonymous.deliveromobile",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FF6B00"
      },
      "config": {
        "googleMaps": {
          "apiKey": "IL_TUO_API_KEY_GOOGLE_MAPS"
        }
      }
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.deliveromobile",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Delivero ha bisogno della tua posizione per mostrare i ristoranti vicini e tracciare le consegne.",
        "NSLocationAlwaysUsageDescription": "Delivero traccia la tua posizione per aggiornare i clienti sullo stato della consegna.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "extra": {
      "eas": {
        "projectId": "4c2ea20f-23f0-4198-ba8e-fa276531983a"
      }
    },
    "owner": "romano98",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Consenti a Delivero di usare la tua posizione."
        }
      ],
      "expo-system-ui"
    ]
  }
}